{
  "env": "gc",
  "debug": true,
  "print_level": "debug",
  "cluster": {
    "discovery": {
      "mode": "nats",
      "@mode1": "mode=default,从profile-{x}.json读取node节点的配置数据",
      "@mode2": "mode=nats,通过nats->master_node_id获取已注册的节点",
      "@mode3": "mode=etcd,通过etcd同步已注册节点"
    },
    "nats": {
      "master_node_id": "gc-master",
      "address": "nats://192.168.1.113:4333,nats://192.168.1.113:4444,nats://192.168.1.113:4555",
      "reconnect_delay": 1,
      "max_reconnects": 0,
      "request_timeout": 2,
      "user": "root",
      "password": "123456"
    }
  },
  "node": {
    "master": [
      {
        "node_id": "gc-master",
        "address": "",
        "__settings__": {
          "ref_logger": "master_log"
        },
        "enable": true
      }
    ],
    "center": [
      {
        "node_id": "gc-center",
        "address": "",
        "__settings__": {
          "db_id_list" : {
            "default": "center"
          },
          "ref_logger": "center_log"
        },
        "enable": true
      }
    ],
    "gate": [
      {
        "node_id": "gc-gate-1",
        "address": ":10010",
        "__settings__": {
          "ref_logger": "gate_log"
        },
        "enable": true
      },
      {
        "node_id": "gc-gate-2",
        "address": ":10020",
        "__settings__": {
          "ref_logger": "gate2_log"
        },
        "enable": true
      }
    ],
    "web": [
      {
        "node_id": "gc-web-1",
        "address": "0.0.0.0:10000",
        "__settings__": {
          "ref_logger": "web_log",
          "md5_key":""
        },
        "enable": true
      }
    ],
    "game": [
      {
        "node_id": "1",
        "__settings__": {
          "db_id_list" : {
            "default": "game_1",
            "log": "log_1"
          },
          "ref_logger": "game1_log"
        },
        "enable": true
      },
      {
        "node_id": "2",
        "__settings__": {
          "db_id_list" : {
            "default": "game_2",
            "log": "log_2"
          },
          "ref_logger": "game2_log"
        },
        "enable": true
      }
    ]
  },
  "logger": {
    "master_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/master.log",
      "file_path_format": "logs/master_%Y%m%d%H%M.log"
    },
    "center_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/center.log",
      "file_path_format": "logs/center_%Y%m%d%H%M.log"
    },
    "web_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/web.log",
      "file_path_format": "logs/web_%Y%m%d%H%M.log"
    },
    "gate_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/gate.log",
      "file_path_format": "logs/gate_%Y%m%d%H%M.log"
    },
    "gate2_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/gate2.log",
      "file_path_format": "logs/gate2_%Y%m%d%H%M.log"
    },
    "game1_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/game1.log",
      "file_path_format": "logs/game1_%Y%m%d%H%M.log"
    },
    "game2_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/game2.log",
      "file_path_format": "logs/game2_%Y%m%d%H%M.log"
    },
    "cross_log": {
      "level": "debug",
      "enable_console": true,
      "enable_write_file": true,
      "time_format" : "2006-01-02 15:04:05.000",
      "file_link_path": "logs/cross.log",
      "file_path_format": "logs/cross_%Y%m%d%H%M.log"
    }
  },
  "data_config": {
    "parser": "json",
    "data_source": "file",
    "file": {
      "file_path": "data/",
      "ext_name": ".json",
      "reload_time": 3000
    },
    "redis": {
      "address": "192.168.1.113:6379",
      "password": "",
      "db": 7
    }
  },
  "db": {
    "center_db_group": [
      {
        "enable": true,
        "db_id": "center",
        "dsn" : "",
        "db_name": "dhc_x3_center",
        "host": "192.168.1.113",
        "user_name": "root",
        "password": "123456",
        "max_idle_connect": 10,
        "max_open_connect": 100,
        "log_mode": true
      }
    ],
    "game_db_group": [
      {
        "enable": true,
        "db_id": "game_1",
        "dsn" : "",
        "db_name": "dhc_x3_game_1",
        "host": "192.168.1.113",
        "user_name": "root",
        "password": "123456",
        "max_idle_connect": 10,
        "max_open_connect": 100,
        "log_mode": true
      },
      {
        "enable": true,
        "db_id": "log_1",
        "dsn" : "%s:%s@(%s)/%s?charset=utf8&parseTime=True&loc=Local&timeout=5s&multiStatements=true",
        "db_name": "dhc_x3_log_1",
        "host": "192.168.1.113",
        "user_name": "root",
        "password": "123456",
        "max_idle_connect": 10,
        "max_open_connect": 100,
        "log_mode": true
      },
      {
        "enable": true,
        "db_id": "game_2",
        "dsn" : "",
        "db_name": "dhc_x3_game_2",
        "host": "192.168.1.113",
        "user_name": "root",
        "password": "123456",
        "max_idle_connect": 10,
        "max_open_connect": 100,
        "log_mode": true
      },
      {
        "enable": true,
        "db_id": "log_2",
        "dsn" : "%s:%s@(%s)/%s?charset=utf8&parseTime=True&loc=Local&timeout=5s&multiStatements=true",
        "db_name": "dhc_x3_log_2",
        "host": "192.168.1.113",
        "user_name": "root",
        "password": "123456",
        "max_idle_connect": 10,
        "max_open_connect": 100,
        "log_mode": true
      }
    ]
  }
}